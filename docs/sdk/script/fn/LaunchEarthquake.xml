  <func>
    <title>LaunchEarthquake</title>
    <category>Umwelt</category>
    <version>4.6.5.0 CP</version>
    <syntax>
      <rtype>bool</rtype>
      <params>
        <param>
          <type>int</type>
          <name>x</name>
          <desc>X-Zielposition</desc>
        </param>
        <param>
          <type>int</type>
          <name>y</name>
          <desc>Y-Zielposition</desc>
        </param>
