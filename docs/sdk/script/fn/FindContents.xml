<funcs>
  <func>
    <title>FindContents</title>
    <category>Objects</category>
    <subcat>Search</subcat>
    <version>1.0 OC</version>
    <syntax>
      <rtype>object</rtype>
      <params>
        <param>
          <type>id</type>
          <name>type</name>
          <desc>Object type to be found.</desc>
        </param>
